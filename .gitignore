# See https://help.github.com/articles/ignoring-files for more about ignoring files.

# Bundler config
/.bundle

# Environment variable files
.env*
/config/master.key

# Log and temp files
/log/*
!/log/.keep
/tmp/*
!/tmp/.keep

# PIDs
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Active Storage (uploaded files)
/storage/*
!/storage/.keep
/tmp/storage/*
!/tmp/storage/
!/tmp/storage/.keep

# Public assets (precompiled)
/public/assets

# Asset builds
/app/assets/builds/*
!/app/assets/builds/.keep

# Vendor directory (optional)
vendor/

# === Editor and OS junk ===
.idea/
.vscode/
.DS_Store
Thumbs.db
*.swp

# Byebug
.byebug_history

# Node (if using Tailwind via npm or yarn)
/node_modules/
yarn.lock
package-lock.json

# Docker
*.log
*.pid
*.sock
*.db
docker-compose.override.yml

.kamal/secrets/KAMAL_REGISTRY_PASSWORD

